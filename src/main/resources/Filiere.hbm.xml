<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.Filiere" table="filiere" schema="fpl2023">
        <id name="idFiliere">
            <column name="idFiliere" sql-type="int"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="libelle">
            <column name="libelle" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>